	if (btn (1)) then xspeed=speed end
 if (btn (2)) then yspeed=-speed end
	if (btn (3)) then yspeed=speed end
	if (btnp(4)) and z==0 then zspeed=speed sfx(0,1)  end
	
	if(pget(x+xspeed,y+yspeed)!=6 or z!=0) then
		--if(xspeed!=0) then
			--if(step==0) then step=1
				--else step=0 end
		--end
		x+=xspeed y+=yspeed
	end
