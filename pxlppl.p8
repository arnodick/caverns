x=32 y=32 z=0 c1=14 c2=2
w=128 h=128
speed=0.65 xspeed=0 yspeed=0 zspeed=0
step=0

function makeactor(x,y,w,c1,c2)
	local a={}
	a.x=x
	a.y=y
	a.w=w
	a.c1=c1
	a.c2=c2
	add(actor,a)
	return a
end

function drawwall(x,y,w,c1,c2)
	rectfill(x,y,x+w,y+w,c1)
 rectfill(x,y+w+1,x+w,y+w+1,c2)
end

function _init()
	--mgen()
	actor={}
	--mset(16,16,8)
	makeactor(1,50,14,6,9)
	makeactor(1,2,0,8,9)
end

function _draw()
	cls()
	rectfill(0,0,w,h,13)
	map(0,0,0,0,w,h)
	camera()
	--spr(1,x,y)
	
	if(z<0) then
	 pset(x,y+1,5)
	end
	if pget(x,y+z+1)==13 then
  pset(x,y+z+1,c2)
 end
 
 for i=1,count(actor) do
 	drawwall(actor[i].x,actor[i].y,actor[i].w,actor[i].c1,actor[i].c2)
 end
 pset(x,y+z,c1)
 pset(16,16,mget(16,16))
	--print(s,x+2,y+2)
end

function _update()
	xspeed=0 yspeed=0
	if (btn (0)) then xspeed=-speed end
	if (btn (1)) then xspeed=speed end
 if (btn (2)) then yspeed=-speed end
	if (btn (3)) then yspeed=speed end
	if (btnp(4)) and z==0 then zspeed=speed sfx(0,1)  end
	if (btn (5)) then makeactor(x+5,y+5,2,4,15) end
	
	if(pget(x+xspeed,y+yspeed)!=6 or z!=0) then
		x+=xspeed y+=yspeed
	end
	z-=zspeed
	if(z<0) then zspeed-=0.1
	 else z=0
	end
end





