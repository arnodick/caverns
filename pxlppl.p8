 pset(x,y+z,c1)
 pset(16,16,mget(16,16))
 --rectfill(x,y,x+1,y-1,c1)
 --rectfill(x,y+1,x+1,y+1,c2)
	--print(s,x+2,y+2)
end

function _update()
	xspeed=0 yspeed=0
