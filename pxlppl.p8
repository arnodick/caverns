function tileflag(x,y,f)
	x=x/8 y=(y/8)+1
 return fget(mget(x,y),f)
end

function makewall(x,y,w)
	rectfill(x,y,x+w,y+w,6)
 rectfill(x,y+w+1,x+w,y+w+1,5)
end

function _init()
	--mgen()
	mset(16,16,8)
end

function _draw()
	cls()
	rectfill(0,0,w,h,13)
	map(0,0,0,0,w,h)
	camera()
	--spr(1,x,y)
	
	if(z<0) then
	 pset(x,y+1,5)
	end
	if pget(x,y+z+1)==13 then
  pset(x,y+z+1,c2)
 end
	makewall(45,54,4)
